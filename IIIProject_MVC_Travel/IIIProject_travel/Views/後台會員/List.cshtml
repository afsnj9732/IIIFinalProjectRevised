@{
    Layout = null;
}
@model IEnumerable<tMember>

<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Mark Otto, Jacob Thornton, and Bootstrap contributors">
    <meta name="generator" content="Jekyll v3.8.5">
    <title>Dashboard Template · Bootstrap 4 繁體中文手冊 [六角學院譯]</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="~/Content/css/dashboard.css" rel="stylesheet" />
    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
        }
    </style>

</head>

<body>
    @using (Html.BeginForm())
    {
        <nav class="navbar navbar-dark fixed-top bg-dark flex-md-nowrap p-0 shadow">
            <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="#">Company name</a>
            <input type="text" placeholder="關鍵字" aria-label="Search" name="txt關鍵字" class="form-control form-control-dark w-100">
            <input type="submit" value="搜尋" class=" btn btn-dark" />
            <ul class="navbar-nav px-3">
                <li class="nav-item text-nowrap">
                    <a>
                        @Html.ActionLink("Sign out", "Home", "Home", new { id = 0 }, new { @class = "nav-link" })
                    </a>
                </li>
            </ul>
        </nav>

    }

    <div class="container-fluid">
        <div class="row">
            <nav class="col-md-2 d-none d-md-block bg-light sidebar">
                <div class="sidebar-sticky">
                    <ul class="nav flex-column">
                        <li class="nav-item">
                            <a class="nav-link active" href="@Url.Action("Home","後台Home")">
                                <span data-feather="home"></span>
                                首頁 <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("List","後台會員")">
                                <span data-feather="users"></span>
                                會員列表
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("List","後台旅遊")">
                                <span data-feather="file"></span>
                                旅遊列表
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("List","後台飯局")">
                                <span data-feather="bar-chart-2"></span>
                                飯局列表
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("List","後台文章")">
                                <span data-feather="layers"></span>
                                文章列表
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("List","聯絡我們")">
                                <span data-feather="layers"></span>
                                意見列表
                            </a>
                        </li>

                    </ul>

                </div>
            </nav>

            <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">


                <h2>會員列表</h2>
                <div class="table-responsive">
                    <table class="table table-striped table-sm">
                        <thead>
                            <tr>
                                <td>會員編號</td>
                                <td>名稱</td>
                                <td>評分</td>
                                <td>稱號</td>
                                <td>大頭貼</td>
                                <td>帳號</td>
                                <td>密碼</td>
                                <td>電子郵件</td>
                                <td>手機</td>
                                <td>電話</td>
                                <td>生日</td>
                                <td>自我介紹</td>
                                <td>暱稱</td>
                                <td>英文名稱</td>
                                <td>性別</td>
                                <td>興趣</td>
                                <td>按鈕</td>
                                <td>按鈕</td>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員編號)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員名稱)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員評分)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員稱號)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員大頭貼)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員帳號)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員密碼)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員電子郵件)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員手機)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員電話)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員生日)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員自我介紹)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員暱稱)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員英文姓名)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員性別)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.f會員興趣)
                                    </td>
                                    <td>
                                        @Html.ActionLink("編輯", "e修改", "後台會員", new { id = item.f會員編號 }, new { Class = "btn btn-info" })
                                    </td>
                                    <td>
                                        @Html.ActionLink("刪除", "d刪除", "後台會員", new { id = item.f會員編號 },
                                        new { OnClick = "return confirm('確定要刪除嗎?')", Class = "btn btn-danger" })
                                    </td>

                                </tr>

                            }

                        </tbody>
                    </table>
                </div>
            </main>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
            integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
            crossorigin="anonymous"></script>
    <script>window.jQuery || document.write('<script src="~/Scripts/jquery-slim.min.js"><\/script>')</script>
    <script src="~/Content/bootstrap-4.5.2-dist/js/bootstrap.bundle.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/feather-icons/4.9.0/feather.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.min.js"></script>
    <script src="~/Scripts/IIIProject/dashboard.js"></script>

</body>

</html>